{
   "content_scripts": [ {
      "js": [ "gmail_ext.js", "anywhere.js","jquery.js" ],
      "matches": [ "http://mail.google.com/*", "https://mail.google.com/*","http://inbox.google.com/*", "https://inbox.google.com/*"  ],
      "run_at": "document_end"
   } ],
   "description": "Todoist for Inbox is a customized Extension from Todoist. Todoist for Inbox is not from the Todoist Company !",
   "icons": {
      "128": "todoist_128.png",
      "16": "todoist_16.png",
      "256": "todoist_256.png",
      "48": "todoist_48.png"
   },

   "manifest_version": 2,
   "name": "Todoist for Inbox",
   "permissions": [ "http://*.todoist.com/*", "https://*.todoist.com/*", "https://mail.google.com/*", "http://mail.google.com/*","https://inbox.google.com/","http://inbox.google.com/" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0",
   "web_accessible_resources": [ "frame.html" ]
}
